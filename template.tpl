const createArgumentsQueue = require('createArgumentsQueue');
const log = require('logToConsole');
const makeTableMap = require('makeTableMap');
const setDefaultConsentState = require('setDefaultConsentState');
const updateConsentState = require('updateConsentState');
const gtag = createArgumentsQueue('gtag', 'dataLayer');
const injectScript = require('injectScript');
const gtagSet = require('gtagSet');

const setCookie = require('setCookie');
const queryPermission = require('queryPermission');

const createQueue = require('createQueue');
const dataLayerPush = createQueue('dataLayer');

const getCookieValues = require('getCookieValues');
// pixel
const sendPixel = require('sendPixel');
const encodeUriComponent = require('encodeUriComponent');


const onSuccess = () => {
  data.gtmOnSuccess();
};

const onFailure = () => {
  data.gtmOnFailure();
};

let doInit = false;
let doUpdate = false;

//read cookie value
//& launch banner if no value found
//if consent value found -> cookie_consent_update
const cookieName = 'consent_client';
let cookieValues;
if (queryPermission('get_cookies', cookieName)) {
  cookieValues = getCookieValues(cookieName);
  
  //debugging: datalayer PUSH show cookie value
   const eventName2 =  {'consent_client' :  cookieValues };
   dataLayerPush(eventName2);
  
  if(cookieValues == '' || cookieValues == null || cookieValues == '[]' || cookieValues == undefined ){
  
     //no consent yet -> launch banner
     //debugging: datalayer PUSH show cookie value
      const eventName3 =  {'consent_client' :  'needsApproval' };
      dataLayerPush(eventName3);
  
    
    
    
      //show banner
        const eventNamex1 =  {'event' :  data.theme };
         dataLayerPush(eventNamex1);
            const eventNamex2 =  {'event' :  data.language };
         dataLayerPush(eventNamex2);
    
      //dark banner
      if (data.theme === 'dark') {
        if (data.language === 'finnish') {
          injectScript('https://app.consentbanner.com/cookies-banner-dark-finnish.js', onSuccess, onFailure);
        }
        if (data.language === 'english') {
          injectScript('https://app.consentbanner.com/cookies-banner-dark-english.js', onSuccess, onFailure);
        }
     }
    
     //light banner
     else if (data.theme === 'light') {
        if (data.language === 'finnish') {
          injectScript('https://app.consentbanner.com/cookies-banner-light-finnish.js', onSuccess, onFailure);
        }
        if (data.language === 'english') {
          injectScript('https://app.consentbanner.com/cookies-banner-light-english.js', onSuccess, onFailure);
        }
     }
    
     //default banner
      else {
        injectScript('https://app.consentbanner.com/cookies-banner.js', onSuccess, onFailure);
     }
    
    
    //show banner ends
    
    
    
        //gtagSet('developer_id.dNmIyNz', true);
     
   } 
  //if ends
  
  else{
     //cookie is set already and consent given:
     doUpdate = true;
     //datalayer PUSH -> trigger: read consent_values and trigger for all tracking tags
     //const eventName =  {'event' :  'cookie_consent_update' };
     //dataLayerPush(eventName);
     //datalayer PUSH ends
  }
    

} 
//read cookie value & launch banner ends




//if (data.url_passthrough) gtagSet('url_passthrough', true);
//if (data.ads_data_redaction) gtagSet('ads_data_redaction', true);

// INIT -----------------------------------------------------------------------------------
if (data.eventValue === 'init') {
  data.settingsTable.forEach(setting => {
    const settingObject = {
      ad_storage: setting.ad_storage,
      analytics_storage: setting.analytics_storage,
      functionality_storage: setting.functionality_storage,
      personalization_storage: setting.personalization_storage,
      security_storage: setting.security_storage,
      wait_for_update: setting.wait_for_update,
    };
    const tagManagerConsent = {
      ad_storage: setting.ad_storage,
      analytics_storage: setting.analytics_storage,
      functionality_storage: setting.functionality_storage,
      personalization_storage: setting.personalization_storage,
      security_storage: setting.security_storage
    };
    if (setting.regions !== 'all') {
      settingObject.region = setting.regions.split(',').map(r => r.trim());
      tagManagerConsent.region = setting.regions.split(',').map(r => r.trim());
    }
    gtag('consent', 'default', settingObject);
    setDefaultConsentState(settingObject);    
  });
  
  
  
     //datalayer PUSH -> trigger: read consent_values and trigger for all tracking tags
     //const eventName =  {'event' :  'cookie_consent_update' };
     //dataLayerPush(eventName);
     //datalayer PUSH ends
  
  
  
}



// UPDATE -----------------------------------------------------------------------------------
if (data.eventValue === 'update' || doUpdate == true) {
  

  
  //read current consent from cookie (name: consent_client). Cookie is set by banner.js
  if (queryPermission('get_cookies', cookieName)) {
      cookieValues = getCookieValues(cookieName);
  }
  
  if(cookieValues=='allowAll'){
  gtag('consent', 'update', {
    ad_storage: 'granted',
    analytics_storage: 'granted',
    functionality_storage: 'granted',
    personalization_storage: 'granted',
    security_storage: 'granted'
  });
  updateConsentState({
    ad_storage: 'granted',
    analytics_storage: 'granted',
    functionality_storage: 'granted',
    personalization_storage: 'granted',
    security_storage: 'granted'
  });
  }
  
  if(cookieValues=='acceptNecessary'){
  gtag('consent', 'update', {
    ad_storage: 'denied',
    analytics_storage: 'granted',
    functionality_storage: 'granted',
    personalization_storage: 'granted',
    security_storage: 'granted'
  });
  updateConsentState({
    ad_storage: 'denied',
    analytics_storage: 'granted',
    functionality_storage: 'granted',
    personalization_storage: 'granted',
    security_storage: 'granted'
  });
  }
  
     //datalayer PUSH -> trigger: read consent_values and trigger for all tracking tags
     const eventName =  {'event' :  'cookie_consent_update' };
     dataLayerPush(eventName);
     //datalayer PUSH ends
  
  
  
  //send client usage data 
  // capture values of url: path, fragment..empty gives full url
  const getUrlx = require('getUrl');
  var urlvalue = getUrlx();

//Send pixel to API
//const url = 'https://data.flb7.com/secure/in.php?u=' + encodeUriComponent(urlvalue) + '&ty=' + trackType+ '&tip=' + ipAddress + '&ug=' + userAgent + '&td=' + tDevice;
const ea = data.emailAddress;  
const url = 'https://app.consentbanner.com/' + '?url=' + encodeUriComponent(urlvalue) +  '&ea=' + ea;
sendPixel(url, data.gtmOnSuccess, data.gtmOnFailure);
  
  
    
}
//-----------------------------------------------------------------

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


